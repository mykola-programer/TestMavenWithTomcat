<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>ua.nike.learn.HelloServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Principles_OOP</servlet-name>
    <servlet-class>ua.nike.learn.Principles_OOP</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Principles_OOP</servlet-name>
    <url-pattern>/principles_oop</url-pattern>
  </servlet-mapping>

</web-app>
